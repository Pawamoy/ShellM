#!/bin/bash
## \usage xrun script args
## \brief Load shellm then run a script.
dir="$(dirname "$0")"
# shellcheck disable=SC2164
dir="$(cd "$dir"/..; pwd)"
export shellm="$dir"
# shellcheck source=usr/shellmrc
. "${shellm}/usr/shellmrc"

script=$1; shift
# shellcheck disable=SC1090
. "${script}" "$@"
