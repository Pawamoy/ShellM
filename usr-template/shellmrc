# Path variables ---------------------------------------------------------------
if ! echo "${PATH}" | grep -q "${shellm}"; then
  export PATH="${shellm}/usr/bin:${shellm}/bin:${PATH}"
fi

MANPATH="${shellm}/usr/man:${shellm}/man:"
export LIBPATH="${shellm}/usr/lib:${shellm}/lib"

# Core libraries ---------------------------------------------------------------
. "${shellm}/lib/core/include.sh"
include "core/shellman.sh"
include "core/init.sh"
include "core/utils.sh"

# User environment -------------------------------------------------------------
#include "something.sh"
